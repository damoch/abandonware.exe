<Window x:Class="abandonware.exe.CDKeyCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:abandonware.exe"
        mc:Ignorable="d"
        Title="CD-Key check" Height="118.655" Width="404.848">
    <Grid Margin="0,0,0,4">
        <Label Content="Enter CD-Key (check the back of CD case)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="301"/>
        <TextBox x:Name="cdkeyTextBox"  HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="221" />
        <Button Content="Apply" HorizontalAlignment="Left" Margin="277,44,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
